<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="154.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="vgtu.ignas.view.controller.GroupController">
   <children>
      <GridPane prefWidth="600.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Study Programs ID" />
            <Label text="Group Code" GridPane.rowIndex="1" />
            <Label text="Study Form" GridPane.rowIndex="2" />
            <Label text="Year" GridPane.rowIndex="3" />
            <TextField fx:id="SpId" promptText="ID" GridPane.columnIndex="1" />
            <TextField fx:id="code" promptText="code" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="year" promptText="year" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Button mnemonicParsing="false" onAction="#saveGroup" text="Save" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <ComboBox fx:id="form" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
         </children>
      </GridPane>
   </children>
</VBox>
